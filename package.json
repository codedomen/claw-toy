    "expo-build-properties": "~1.0.9",
    "expo-file-system": "^19.0.14",
    "expo-font": "~14.0.8",
    "expo-gl": "~16.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
